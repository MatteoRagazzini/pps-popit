<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            stylesheets="/css/style.css"
            fx:id="savedTracksPane"
            fx:controller="view.controllers.SavedTracksController"
            prefHeight="400.0" prefWidth="600.0">
    <center>
            <VBox spacing="50">
                    <HBox fx:id="titleLogo" alignment="TOP_CENTER" maxHeight="50"/>
                <ScrollPane hbarPolicy="NEVER" vbarPolicy="ALWAYS" fitToHeight="true" fitToWidth="true" >
                    <FlowPane fx:id="flowPaneTracks" hgap="50" vgap="50" alignment="CENTER">
                    </FlowPane>
                </ScrollPane>
            </VBox>
    </center>
</BorderPane>
