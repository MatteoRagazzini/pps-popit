<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="gameMenu" styleClass="gameMenu" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controllers.GameMenuController">
    <HBox fx:id="inputButtons" styleClass="inputBox">
        <ToggleButton fx:id="playButton" mnemonicParsing="false" styleClass="inputButton" />
        <ToggleButton fx:id="exitButton" mnemonicParsing="false" styleClass="inputButton" />
    </HBox>
    <HBox fx:id="gameStatus" prefHeight="100.0" prefWidth="200.0" styleClass="inputBox" />
    <VBox fx:id="towerDepot" prefHeight="200.0" prefWidth="100.0" styleClass="inputBox" />
</VBox>
